<rxNav:NavigationWindow
    x:Class="CrissCross.WPF.Test.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:CrissCross.WPF.Test"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:rxNav="https://github.com/reactivemarbles/CrissCross"
    x:Name="mainWindow"
    Title="MainWindow"
    Width="800"
    Height="450"
    x:TypeArguments="local:MainWindowViewModel"
    Style="{DynamicResource SplitNavigationWindowStyle}"
    mc:Ignorable="d">
    <Grid>
        <Button
            x:Name="NavBack"
            Height="30"
            Content="Back" />
    </Grid>
</rxNav:NavigationWindow>
