<Application
    x:Class="CrissCross.WPF.UI.CC_Nav.Test.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:CrissCross.WPF.UI.CC_Nav.Test"
    xmlns:rxNav="https://github.com/ChrisPulman/CrissCross.ui"
    StartupUri="MainWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <rxNav:ControlsDictionary />
                <rxNav:ThemesDictionary Theme="Light" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
